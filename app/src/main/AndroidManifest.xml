<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.test.a3dclock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.ClockViewActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>

        <activity android:name=".activity.RoateTestAtivity"/>
        <activity android:name=".activity.TouchRoateAtivity"/>
        <activity
            android:name=".activity.ToucheClockActivity"
            android:configChanges="keyboardHidden|orientation|screenSize"/>
    </application>

</manifest>